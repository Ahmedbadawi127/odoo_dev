    <record id="doctors_tree_view" model="ir.ui.view">
        <field name="name">lab.doctors.tree</field>
        <field name="model">lab.doctors</field>
        <field name="arch" type="xml">
            <tree string="Doctors">
                <field name="seq_doc_no" readonly="1"/>
                <field name="name"/>
<!--                <field name="count_cases"/>-->
<!--                <field name="age"/>-->
                <field name="age_now"/>
                <field name="address"/>
                <field name="gender"/>
                <field name="landline"/>
                <field name="mobile_1"/>
                <field name="mobile_2"/>
                <field name="email"/>
                <field name="governorate"/>
                <field name="city"/>
<!--                <field name="count_cases"/>-->
                <field name="total_billing" invisible="1"/>
                <field name="total_received" invisible="1"/>
                <field name="total_remaining" invisible="1"/>
            </tree>
        </field>
    </record>

    <record id="action_agencies_doctors" model="ir.actions.act_window">
        <field name="name">Doctors</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">lab.doctors</field>
        <field name="view_mode">tree,form</field>
         <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('doctors_tree_view')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('doctors_form_view')})]"/>
    </record>

