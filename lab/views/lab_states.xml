<?xml version="1.0" encoding="UTF-8" ?>

<odoo>


        <record id="lab_cases_registration_tree" model="ir.ui.view">
            <field name="Cases in Registration"></field>
            <field name="model">lab.cases</field>
            <field name="arch" type="xml">
                <tree string="Cases in Registration">
                     <field name="seq_case_no" readonly="1"/>
                    <field name="patient"/>
                    <field name="shade"/>
                    <field name="entry_date"/>
                    <field name="delivery_date"/>
                    <field name="doctor"/>
                    <field name="price"/>
                    <field name="discount"/>
                    <field name="billed_price" readonly="1"/>
                    <field name="estimated_cost"/>
                    <field name="estimated_profit" readonly="1"/>

                </tree>
            </field>
        </record>

