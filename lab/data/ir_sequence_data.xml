<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_lab_doctors" model="ir.sequence">
            <field name="name">Doctors Numbers</field>
            <field name="code">lab.doctors.sequence</field>
            <field name="prefix">Dr-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_lab_cases" model="ir.sequence">
            <field name="name">Cases Numbers</field>
            <field name="code">lab.cases.sequence</field>
            <field name="prefix">case-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>
