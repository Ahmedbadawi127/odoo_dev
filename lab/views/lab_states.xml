    <record id="action_lab_cases_registration" model="ir.actions.act_window">
        <field name="name">Cases in Registration</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">lab.cases</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="lab_cases_registration_tree"/>
<!--        <field name="context">{'search_default_group_Received_books': 1}</field>-->
        <field name="domain">[('state','=','registration')]</field>


    </record>


